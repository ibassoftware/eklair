# RapidAPI TikTok API Configuration
# Get your API key from: https://rapidapi.com/tiktok-api23.p.rapidapi.com
NEXT_PUBLIC_RAPIDAPI_TIKTOK_KEY=your_rapidapi_key_here
NEXT_PUBLIC_RAPIDAPI_TIKTOK_URL=https://tiktok-api23.p.rapidapi.com

# Set to 'true' to use real API, 'false' or omit to use mock data
NEXT_PUBLIC_USE_REAL_API=false

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Authentication
ADMIN_PASSWORD=your_secure_password_here
SESSION_SECRET=your_random_32_char_string_for_jwt_signing
